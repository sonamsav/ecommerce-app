{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "~assets/*": [
                "src/assets/*"
            ],
            "~/*": [
                "src/*"
            ],
            "~containers/*": [
                "src/containers/*"
            ],
            "~pages/*": [
                "src/pages/*"
            ],
            "~api/*": [
                "src/api/*"
            ],
            "~constants/*": [
                "src/constants/*"
            ],
            "~components/*": [
                "src/components/*"
            ],
            "~redux/*": [
                "src/redux/*"
            ],
            "~view profile/*": [
                "src/pages/view profile/*"
            ]
        }
    }
}